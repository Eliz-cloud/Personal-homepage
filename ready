function ssylka() {
  var style ="style2.css" ;
  var current = document.getElementById("myssylka").href;
  
  if (current.endsWith(style)) {
    document.getElementById("myssylka").href="style.css";
  }
  else {
    document.getElementById("myssylka").href="style2.css"; 
  }
}
</script>
